{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "lint:fix": "eslint --ext .js,.ts . --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "genkit start -- npx tsx --watch src/index.ts",
    "dev:terminal": "tsx src/terminal.ts",
    "test:order-agent": "GOOGLE_GENAI_API_KEY=$GOOGLE_GENAI_API_KEY node tests/test-order-agent.js",
    "test:menu-agent": "GOOGLE_GENAI_API_KEY=$GOOGLE_GENAI_API_KEY node tests/test-menu-agent.js",
    "test:simple": "GOOGLE_GENAI_API_KEY=$GOOGLE_GENAI_API_KEY node tests/test-script.js",
    "test:all": "npm run test:simple && npm run test:order-agent && npm run test:menu-agent",
    "test": "npm run test:all"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@genkit-ai/firebase": "^1.18.0",
    "@genkit-ai/googleai": "^1.18.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.0.1",
    "genkit": "^1.18.0",
    "google-auth-library": "^9.6.3",
    "llm-chunk": "^0.0.1",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/pdf-parse": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "csv-parse": "^5.5.6",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "genkit-cli": "^1.18.0",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "private": true
}
