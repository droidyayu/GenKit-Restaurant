# Gradle Properties Sample
# Copy this file to gradle.properties and fill in your actual values
# DO NOT commit gradle.properties to version control

# Android Gradle Plugin Configuration
android.useAndroidX=true
android.enableJetifier=true
kotlin.code.style=official

# API Keys (replace with your actual keys)
RESTAURANT_API_KEY=your-restaurant-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# Build Configuration
org.gradle.jvmargs=-Xmx2048m -Dfile.encoding=UTF-8
org.gradle.parallel=true
org.gradle.caching=true